{# {% if role == 'silai_ho_admin' or role == 'administrator' or role == 'silai_ho_user' or role == 'pc' or role == 'ngo_admin' %} #}
<input type="hidden" id="sewing_dashboard_user_role" value="{{ role }}">
<div class="row" id="sewing_dashboard_section_sa">
	<div class="col-lg-12 col-sm-12">
		<!-- Student Chart Section Start-->
		<div class="panel no-students" id="student_chart_main_sa"> 
			<div class="panel-body">
				<div id="student_chart_inner_section_sa"> 
					<div class="floatl">
						<div class="student-count" id="student-data-count"></div>
						<div class="student-head">Number of Student </div>
					</div>
					<div class="filter-location floatr form-group">
						<select name="school_code" id="school_code" class="form-control">
							<option value=''>{{ schoolCode }}</option>
						</select>
						<select id="student_fy_filter" class="form-control"></select> 
					</div>
					<div class="clearfix"></div>
					<div class="col-lg-12">
						<div id="student_chart"></div> 
						<!--div id="schooltype_info"></div--> 
					</div>
					
				</div>
			</div>
		</div>
		
	</div>
</div>



<div class="row">
	<div class="col-lg-9 col-sm-12">
		<div class="gallery-section-sa">
			<h2>Gallery</h2>
			{{ sewingViewGalleryRender }}
		</div>
	</div>

	<div class="col-lg-3 col-sm-12">
		<div class="notice-board-section-sa">
			<h2>Notice Board</h2>
			{{ viewNoticeRender }}
		</div>
	</div>
</div>
{# {% else %}
	<script type="text/javascript">
		window.location = "/";
	</script>
{% endif %}
 #}